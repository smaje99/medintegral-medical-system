<mjml>
  <mj-head>
    <mj-attributes>
      <mj-all padding="0px"></mj-all>
      <mj-class name="preheader" color="#000000" font-size="11px" font-family="Ubuntu, Helvetica, Arial, sans-serif" padding="0px"></mj-class>
    </mj-attributes>
    <mj-style inline="inline">a { text-decoration: none; }</mj-style>
    <mj-font name="Roboto" href="https://fonts.googleapis.com/css2?family=Roboto&display=swap" />
    <mj-font name="Oswald" href="https://fonts.googleapis.com/css2?family=Oswald&display=swap" />
  </mj-head>
  <mj-body background-color="#dee5e8">
    <mj-section background-color="#C2F2DC" padding="10px 0">
      <mj-column width="33%">
        <mj-image src="https://raw.githubusercontent.com/smaje99/medintegral-medical-system/main/frontend/public/assets/icons/medintegral.svg" alt="medintegral logo" width="150px" padding="10px 0"></mj-image>
      </mj-column>
      
      <mj-column width="66%">
        <mj-text align="left" color="#01a95a" font-size="20px" font-family="Roboto" padding="10px 20px">
          Medicina domiciliaria
        </mj-text>
      </mj-column>
    </mj-section>
    
    <mj-section background-color="#ffffff" padding-top="20px">
      <mj-column width="100%">       
        <mj-text align="center" color="#111" font-size="18px" font-family="Roboto" padding="100px 0px">
       		Test email for: {{ email }}
        </mj-text>
      </mj-column>
    </mj-section>

    <mj-section background-color="#014D29" padding="10px">
      <mj-column vertical-align="top" width="100%">
        <mj-text align="center" color="#dfe6e9" font-size="20px" font-family="Oswald" padding="10px 25px" letter-spacing="1px" line-height="1.5">
          <strong>
          	MEDICINA INTEGRAL DEL CAQUET√Å IPS S.A.S
          </strong>
        </mj-text>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>